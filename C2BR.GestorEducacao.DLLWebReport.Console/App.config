<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true"/>
  </system.web>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="C2BRRelatorioWebBehavior">
          <serviceDebug includeExceptionDetailInFaults="True"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="C2BR.DLLRelatorioWeb.RelatorioWeb" behaviorConfiguration="C2BRRelatorioWebBehavior">
        <endpoint address="net.tcp://localhost:1234/C2BRDLLRelatorioWeb/" binding="netTcpBinding" bindingConfiguration="BindTcpC2BR" name="BindTcpServiceRelatorioWeb" contract="C2BR.DLLRelatorioWeb.Interfaces.IRelatorioWeb">
          <identity>
            <dns value="localhost"/>
          </identity>
        </endpoint>
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding name="BindTcpC2BR" maxBufferSize="65536000" maxConnections="1000" maxReceivedMessageSize="65536000" portSharingEnabled="true">
          <readerQuotas maxStringContentLength="65536000"/>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
