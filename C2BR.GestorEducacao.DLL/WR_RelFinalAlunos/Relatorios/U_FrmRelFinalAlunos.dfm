        'select distinct(A.NO_ALU),A.NU_NIS, A.CO_ALU, MM.CO_ALU_CAD,A.CO' +
        '_SEXO_ALU'
      
        'from TB79_HIST_ALUNO H, TB07_ALUNO A, TB80_MASTERMATR MM, TB02_M' +
