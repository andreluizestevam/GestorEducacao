  inherited QryRelatorio: TADOQuery
    CursorType = ctStatic
    SQL.Strings = (
      '   Select A.CO_AREACON,               '
      '        B.NO_AREACON,                 '
      '        A.NO_ACERVO,                  '
      '        D.NO_AUTOR,                   '
      '        C.NO_EDITORA,                 '
      '        E.DT_EMPR_ACER,               '
      '        F.DT_PREV_DEVO_ACER,          '
      '        F.DT_REAL_DEVO_ACER,          '
      '        F.VL_MULT_ATRA_ACER,          '
      '        E.TP_USUA_ACER,               '
      '        E.CO_USUA_ACER,               '
      '        E.CO_COLA_EMPR_ACER,          '
      '        G.NO_COL                      '
      ' From TB35_ACERVO A,                  '
      '      TB31_AREA_CONHEC B,             '
      '      TB33_EDITORA C,                 '
      '      TB34_AUTOR D,                   '
      '      TB36_EMPR_BIBLIOT  E,           '
      '      TB123_EMPR_BIB_ITENS F,         '
      '      TB03_COLABOR G                  '
      ' Where A.CO_AREACON = B.CO_AREACON    '
      ' AND   A.CO_EDITORA = C.CO_EDITORA    '
      ' AND   A.CO_AUTOR = D.CO_AUTOR        '
      ' AND   A.CO_ACERVO = F.CO_ACERVO      '
      ' AND   F.CO_NUM_EMP = E.CO_NUM_EMP    '
      ' AND   F.TP_USUA_ACER = E.TP_USUA_ACER'
      ' AND   F.CO_USUA_ACER = E.CO_USUA_ACER'
      ' AND   E.CO_COLA_EMPR_ACER = G.CO_COL '
      ''
      '')
  end
end
