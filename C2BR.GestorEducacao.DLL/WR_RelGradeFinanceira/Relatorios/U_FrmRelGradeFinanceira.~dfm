  inherited QryRelatorio: TADOQuery
    CursorType = ctStatic
    SQL.Strings = (
      
        ' SET LANGUAGE PORTUGUESE                                        ' +
        '            '
      
        'SELECT C.CO_TUR, CT.NO_TURMA,C.CO_ALU,C.CO_MODU_CUR,C.CO_CUR,C.C' +
        'O_ANO_MES_MAT,                                                  ' +
        '     '
      
        'm.CO_ALU_CAD, A.NO_ALU,                                         ' +
        '          '
      
        'SITUACAO = (CASE M.CO_SIT_MAT                                   ' +
        '                       '
      
        'WHEN '#39'A'#39' THEN '#39'Aberta'#39'                      WHEN '#39'C'#39' THEN '#39'Cance' +
        'lada'#39'                      '
      
        'WHEN '#39'T'#39' THEN '#39'Trancada'#39'                      WHEN '#39'F'#39' THEN '#39'Fin' +
        'alizada'#39'                      '
      
        'WHEN '#39'R'#39' THEN '#39'Transferido'#39'                      WHEN '#39'D'#39' THEN '#39 +
        'Desistente'#39'                      '
      
        'WHEN '#39'B'#39' THEN '#39'Abandono'#39'                      WHEN '#39'P'#39' THEN '#39'Pen' +
        'dente PG'#39'                      '
      
        'WHEN '#39'M'#39' THEN '#39'Mudan'#231'a de Curso'#39'                      ELSE '#39#39'   ' +
        '                  END),'
      
        'VR_MES_PAR_01 = MAX(CASE C.NU_PAR WHEN 1 THEN C.VR_PAR_DOC END),' +
        '          '
      
        'VR_MES_PAR_02 = MAX(CASE C.NU_PAR WHEN 2 THEN C.VR_PAR_DOC END),' +
        '          '
      
        'VR_MES_PAR_03 = MAX(CASE C.NU_PAR WHEN 3 THEN C.VR_PAR_DOC END),' +
        '          '
      
        'VR_MES_PAR_04 = MAX(CASE C.NU_PAR WHEN 4 THEN C.VR_PAR_DOC END),' +
        '          '
      
        'VR_MES_PAR_05 = MAX(CASE C.NU_PAR WHEN 5 THEN C.VR_PAR_DOC END),' +
        '          '
      
        'VR_MES_PAR_06 = MAX(CASE C.NU_PAR WHEN 6 THEN C.VR_PAR_DOC END),' +
        '          '
      
        'VR_MES_PAR_07 = MAX(CASE C.NU_PAR WHEN 7 THEN C.VR_PAR_DOC END),' +
        '          '
      
        'VR_MES_PAR_08 = MAX(CASE C.NU_PAR WHEN 8 THEN C.VR_PAR_DOC END),' +
        '          '
      
        'VR_MES_PAR_09 = MAX(CASE C.NU_PAR WHEN 9 THEN C.VR_PAR_DOC END),' +
        '          '
      
        'VR_MES_PAR_10 = MAX(CASE C.NU_PAR WHEN 10 THEN C.VR_PAR_DOC END)' +
        ',         '
      
        'VR_MES_PAR_11 = MAX(CASE C.NU_PAR WHEN 11 THEN C.VR_PAR_DOC END)' +
        ',         '
      
        'VR_MES_PAR_12 = MAX(CASE C.NU_PAR WHEN 12 THEN C.VR_PAR_DOC END)' +
        ',         '
      
        'IC_SIT_DOC_01 = MAX(CASE C.NU_PAR WHEN 1 THEN C.IC_SIT_DOC END),' +
        '          '
      
        'IC_SIT_DOC_02 = MAX(CASE C.NU_PAR WHEN 2 THEN C.IC_SIT_DOC END),' +
        '          '
      
        'IC_SIT_DOC_03 = MAX(CASE C.NU_PAR WHEN 3 THEN C.IC_SIT_DOC END),' +
        '          '
      
        'IC_SIT_DOC_04 = MAX(CASE C.NU_PAR WHEN 4 THEN C.IC_SIT_DOC END),' +
        '          '
      
        'IC_SIT_DOC_05 = MAX(CASE C.NU_PAR WHEN 5 THEN C.IC_SIT_DOC END),' +
        '          '
      
        'IC_SIT_DOC_06 = MAX(CASE C.NU_PAR WHEN 6 THEN C.IC_SIT_DOC END),' +
        '          '
      
        'IC_SIT_DOC_07 = MAX(CASE C.NU_PAR WHEN 7 THEN C.IC_SIT_DOC END),' +
        '          '
      
        'IC_SIT_DOC_08 = MAX(CASE C.NU_PAR WHEN 8 THEN C.IC_SIT_DOC END),' +
        '          '
      
        'IC_SIT_DOC_09 = MAX(CASE C.NU_PAR WHEN 9 THEN C.IC_SIT_DOC END),' +
        '          '
      
        'IC_SIT_DOC_10 = MAX(CASE C.NU_PAR WHEN 10 THEN C.IC_SIT_DOC END)' +
        ',         '
      
        'IC_SIT_DOC_11 = MAX(CASE C.NU_PAR WHEN 11 THEN C.IC_SIT_DOC END)' +
        ',         '
      
        'IC_SIT_DOC_12 = MAX(CASE C.NU_PAR WHEN 12 THEN C.IC_SIT_DOC END)' +
        ','
      
        'SUM(C.VR_PAG) VR_TOT_PAG,                                       ' +
        ' '
      
        '                       SUM(C.VR_PAR_DOC) VR_TOT_PAG_DOC         ' +
        '                     '
      
        'FROM TB47_CTA_RECEB C,                                          ' +
        '               '
      
        'TB06_TURMAS T,TB129_CADTURMAS CT,                               ' +
        '          '
      
        'TB07_ALUNO A,                                                   ' +
        '          '
      
        'TB08_MATRCUR M                                                  ' +
        '  '
      
        'WHERE C.CO_EMP = A.CO_EMP                                       ' +
        '           '
      
        'AND  C.CO_EMP = T.CO_EMP                                        ' +
        '          '
      
        'AND  C.CO_CUR = T.CO_CUR                                        ' +
        '          '
      
        'AND  C.CO_TUR = T.CO_TUR                                        ' +
        '          '
      
        'AND CT.CO_MODU_CUR = T.CO_MODU_CUR and CT.CO_TUR = T.CO_TUR     ' +
        '          '
      
        'AND  C.CO_ALU = A.CO_ALU                                        ' +
        '          '
      
        'AND  C.CO_EMP = M.CO_EMP                                        ' +
        '          '
      
        'AND  C.CO_CUR = M.CO_CUR                                        ' +
        '          '
      
        'AND  C.CO_ALU = M.CO_ALU                                        ' +
        '          '
      
        'AND  C.CO_ANO_MES_MAT = M.CO_ANO_MES_MAT                        ' +
        '          '
      
        'AND  C.IC_SIT_DOC <> '#39'C'#39'   AND  C.CO_CUR = 16   AND  C.CO_MODU_C' +
        'UR = 1   '
      
        'AND  C.CO_ANO_MES_MAT = '#39'2009  '#39'   AND  C.NU_SEM_LET = 1    AND ' +
        ' C.CO_EMP = 187 '
      'AND C.CO_TUR = 13  '
      
        'GROUP BY C.CO_TUR,                                 CT.NO_TURMA, ' +
        '                             '
      
        'm.CO_ALU_CAD,                             A.NO_ALU, C.CO_ALU ,C.' +
        'CO_MODU_CUR,C.CO_CUR,C.CO_ANO_MES_MAT,                          ' +
        '     '
      'M.CO_SIT_MAT                     '
      
        'ORDER BY C.CO_TUR,                                 CT.NO_TURMA, ' +
        '                                '
      
        'm.CO_ALU_CAD,                             A.NO_ALU,             ' +
        '                    '
      
        'M.CO_SIT_MAT,C.CO_MODU_CUR,C.CO_CUR,C.CO_ANO_MES_MAT            ' +
        '     '
      '')
