  inherited QryRelatorio: TADOQuery
    SQL.Strings = (
      
        'SET LANGUAGE PORTUGUESE  Select C.*, H.DE_HISTORICO, P.CO_GRUP_C' +
        'TA, P.CO_SGRUP_CTA, '
      'P.CO_CONTA_PC, P.DE_CONTA_PC,    '
      
        'TIPOCLIENTE = (CASE C.TP_CLI_RECDES WHEN '#39'J'#39' THEN '#39'Pessoa Jur'#237'di' +
        'ca'#39'WHEN '#39'F'#39' '
      'THEN '#39'Pessoa F'#237'sica'#39' END),TP.DES_TIPO_DOC,'
      
        'SITUACAO_REC_FIX = (CASE C.IC_SIT_RECDES WHEN '#39'A'#39' THEN '#39'Em Abert' +
        'o'#39#39'#9#9'#39'WHEN '#39'C'#39' '
      'THEN '#39'Cancelado'#39'WHEN '#39'Q'#39' THEN '#39'Quitado'#39' END)  '
      
        'From TB37_RECDES_FIXA C  JOIN TB39_HISTORICO H ON H.CO_HISTORICO' +
        ' = C.CO_HISTORICO  '
      'JOIN TB56_PLANOCTA P ON P.CO_SEQU_PC = C.CO_SEQU_PC  '
      'JOIN TB086_TIPO_DOC TP ON TP.CO_TIPO_DOC = C.CO_TIPO_DOC  '
      
        'Where C.CO_HISTORICO = H.CO_HISTORICO    AND C.CO_SEQU_PC = P.CO' +
        '_SEQU_PC    AND '
      'C.TP_CON_RECDES = '#39'C'#39'   AND C.CO_EMP = 187')
  end
