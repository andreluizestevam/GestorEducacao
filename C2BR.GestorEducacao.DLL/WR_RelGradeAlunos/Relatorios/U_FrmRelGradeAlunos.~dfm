  inherited QryRelatorio: TADOQuery
    SQL.Strings = (
      'SET LANGUAGE PORTUGUESE'
      
        'SELECT mm.co_alu_cad,a.nu_nis,a.no_alu,a.co_sexo_alu,a.dt_nasc_a' +
        'lu,cid.no_cidade,bai.no_bairro,'
      
        #9'   c.no_cur,ct.co_sigla_turma as no_tur,t.co_tur,c.co_cur,r.no_' +
        'resp,r.nu_tele_resi_resp,'
      '       DEFICIENCIA = (CASE A.TP_DEF'
      '                      WHEN '#39'N'#39' THEN '#39'Nenhuma'#39
      '                      WHEN '#39'A'#39' THEN '#39'Auditiva'#39
      '                      WHEN '#39'V'#39' THEN '#39'Visual'#39
      '                      WHEN '#39'F'#39' THEN '#39'F'#237'sica'#39
      '                      WHEN '#39'M'#39' THEN '#39'Mental'#39
      '                      WHEN '#39'I'#39' THEN '#39'M'#250'ltiplas'#39
      '                      WHEN '#39'O'#39' THEN '#39'Outra'#39
      '                      ELSE '#39#39
      '                   END),'
      '       PARENTESCO = (CASE R.DE_GRAU_PAREN'
      '                      WHEN '#39'PM'#39' THEN '#39'Pai/M'#227'e'#39
      '                      WHEN '#39'AV'#39' THEN '#39'Av'#244'/Av'#243#39
      '                      WHEN '#39'IR'#39' THEN '#39'Irm'#227'o/Irm'#227#39
      '                      WHEN '#39'TI'#39' THEN '#39'Tio(a)'#39
      '                      WHEN '#39'PR'#39' THEN '#39'Primo(a)'#39
      '                      WHEN '#39'CN'#39' THEN '#39'Cunhado(a)'#39
      '                      WHEN '#39'TU'#39' THEN '#39'Tutor(a)'#39
      '                      WHEN '#39'OU'#39' THEN '#39'Outros'#39
      '                      ELSE '#39#39
      '                   END)'
      #9'FROM tb07_aluno a '
      #9#9'JOIN tb08_matrcur mm ON mm.co_alu = a.co_alu '
      #9#9'JOIN tb01_curso c ON c.co_cur = mm.co_cur '
      #9#9'JOIN tb06_turmas t ON t.co_tur = mm.co_tur '
      
        '                                JOIN tb129_cadturmas ct ON t.co_' +
        'tur = ct.co_tur'
      #9#9'JOIN tb108_responsavel r ON r.co_resp = a.co_resp '
      #9#9'JOIN TB905_BAIRRO BAI ON BAI.CO_BAIRRO = A.CO_BAIRRO'
      #9#9'JOIN TB904_CIDADE CID ON CID.CO_CIDADE = A.CO_CIDADE')
