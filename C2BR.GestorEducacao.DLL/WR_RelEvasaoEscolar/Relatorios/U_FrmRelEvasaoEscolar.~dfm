  inherited QryRelatorio: TADOQuery
    CursorType = ctStatic
    SQL.Strings = (
      'SET LANGUAGE PORTUGUESE '
      
        '                  select distinct b.no_cur,b.co_cur,b.co_param_f' +
        'reque,b.SEQ_IMPRESSAO '
      '                  from TB01_CURSO b                '
      '                  where b.co_emp = 187'
      '                  and b.co_modu_cur = 1'
      '                  order by b.SEQ_IMPRESSAO')
  end
end
