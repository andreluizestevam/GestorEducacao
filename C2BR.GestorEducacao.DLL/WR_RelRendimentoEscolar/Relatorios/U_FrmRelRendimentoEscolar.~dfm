  inherited QryRelatorio: TADOQuery
    SQL.Strings = (
      
        'SELECT DISTINCT e.co_emp,e.no_fantas_emp as Empresa,a.co_alu,a.n' +
        'o_alu as Aluno,a.no_mae_alu as Mae, '
      
        #9'    a.no_pai_alu as Pai,h.co_ano_ref as Ano,c.co_cur,c.no_cur a' +
        's Curso,t.co_peri_tur as Turno,ct.co_sigla_turma as Turma, '
      
        #9'    m.co_mat,CM.NO_MATERIA as Materia,h.VL_NOTA_BIM1, h.VL_NOTA' +
        '_BIM2,h.VL_NOTA_BIM3,h.VL_NOTA_BIM4,h.VL_MEDIA_FINAL,'
      
        #9#9'h.VL_PROVA_FINAL,c.co_nivel_cur,a.nu_nis,mm.co_alu_cad,mm.co_s' +
        'it_mat,m.qt_carg_hora_mat,h.qt_falta_bim1,h.qt_falta_bim2,'
      #9#9'h.qt_falta_bim3,h.qt_falta_bim4, MM.CO_STA_APROV'
      '      FROM tb079_hist_aluno h '
      
        '    '#9'JOIN tb01_curso      c ON c.co_cur = h.co_cur and h.co_emp ' +
        '= c.co_emp'
      
        #9'    JOIN tb07_aluno      a ON a.co_alu = h.co_alu and h.co_emp ' +
        '= a.co_emp'
      
        #9#9'JOIN tb08_matrcur mm on h.co_alu = mm.co_alu and h.co_emp = mm' +
        '.co_emp'
      
        #9'    JOIN tb06_turmas     t ON t.co_tur = h.co_tur and h.co_emp ' +
        '= t.co_emp '
      'JOIN tb129_cadturmas     ct ON t.co_tur = ct.co_tur '
      
        '  '#9'  JOIN tb02_materia    m ON m.co_mat = h.co_mat and h.co_emp ' +
        '= m.co_emp '
      '  '#9'  JOIN tb25_empresa    e ON e.co_emp = h.co_emp '
      
        '       JOIN TB107_CADMATERIAS CM ON CM.ID_MATERIA = M.ID_MATERIA' +
        ' '
      '      WHERE h.co_emp = 2'
      '        and h.co_ano_ref = '#39'2008'#39'     '
      #9#9'and h.co_alu = 1296  ')
  end
