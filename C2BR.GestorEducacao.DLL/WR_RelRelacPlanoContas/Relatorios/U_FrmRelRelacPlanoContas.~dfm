  inherited QryRelatorio: TADOQuery
    CursorType = ctStatic
    SQL.Strings = (
      'select gc.DE_GRUP_CTA, sc.DE_SGRUP_CTA, p.DE_CONTA_PC '
      '                   from TB56_PLANOCTA p '
      
        '                   join TB53_GRP_CTA gc on gc.CO_GRUP_CTA = p.CO' +
        '_GRUP_CTA '
      
        '                   join TB54_SGRP_CTA sc on p.CO_SGRUP_CTA = sc.' +
        'CO_SGRUP_CTA and p.CO_GRUP_CTA = sc.CO_GRUP_CTA')
  end
