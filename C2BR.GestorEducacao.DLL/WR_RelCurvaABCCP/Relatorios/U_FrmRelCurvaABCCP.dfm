  inherited QryRelatorio: TADOQuery
    SQL.Strings = (
      'SET LANGUAGE PORTUGUESE '
      '                    Select A.CO_FORN, B.NO_FAN_FOR, '
      
        '                    COUNT(A.NU_DOC) TOT_DOC, SUM(A.VR_PAR_DOC) V' +
        'L_TOTAL '
      '                    From TB38_CTA_PAGAR A, TB41_FORNEC B '
      '                    Where A.CO_FORN = B.CO_FORN '
      '                    AND A.CO_EMP = 187'
      'GROUP BY A.CO_FORN, B.NO_FAN_FOR Order by VL_TOTAL desc')
    object QryRelatorioCO_FORN: TIntegerField
      FieldName = 'CO_FORN'
    end
    object QryRelatorioNO_FAN_FOR: TStringField
      FieldName = 'NO_FAN_FOR'
      Size = 80
    end
