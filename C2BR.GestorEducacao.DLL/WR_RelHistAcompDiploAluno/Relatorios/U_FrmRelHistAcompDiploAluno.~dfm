  inherited QryRelatorio: TADOQuery
    SQL.Strings = (
      
        'Select C.*, CD.NO_CIDADE,BA.NO_BAIRRO from TB103_CLIENTE C, TB90' +
        '4_CIDADE CD, TB905_BAIRRO BA'
      
        '               where C.CO_CIDADE *= CD.CO_CIDADE AND C.CO_BAIRRO' +
        ' *= BA.CO_BAIRRO')
  end
