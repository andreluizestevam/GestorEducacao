      '         M.CO_ALU_CAD'
      ' From TB64_SOLIC_ATEND SA,             '
      '      TB66_TIPO_SOLIC TS,              '
      '      TB65_HIST_SOLICIT B,             '
      '      TB07_ALUNO A,                    '
      '      TB01_CURSO C,'
      '      TB08_MATRCUR M,'
      #9'  TB06_TURMAS TUR, TB129_CADTURMAS CT'
