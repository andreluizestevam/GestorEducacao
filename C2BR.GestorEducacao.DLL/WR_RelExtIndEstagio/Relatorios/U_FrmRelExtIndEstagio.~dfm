  inherited QryRelatorio: TADOQuery
    CursorType = ctStatic
    LockType = ltBatchOptimistic
    SQL.Strings = (
      'SET LANGUAGE PORTUGUESE                '
      ' Select  SA.DE_OBS_SOLI, A.NU_NIS,SA.CO_SOLI_ATEN, '
      '         SA.MES_SOLI_ATEN,'
      '         SA.ANO_SOLI_ATEN,'
      '         SA.NU_DCTO_SOLIC,'
      '         SA.DT_SOLI_ATEN,'
      '         B.CO_CUR,'
      '         CT.CO_SIGLA_TURMA as NO_TUR,'
      '         B.CO_TIPO_SOLI,'
      '         SA.DT_PREV_ENTR,              '
      '         B.DT_FIM_SOLI,                '
      '         A.NO_ALU, C.NO_CUR, C.CO_SIGL_CUR,'
      '         TS.NO_TIPO_SOLI,              '
      '         B.VA_SOLI_ATEN, '
      '         B.CO_SITU_SOLI CO_SIT_SOLI,'
      '         M.CO_ALU_CAD'
      ' From TB64_SOLIC_ATEND SA,             '
      '      TB66_TIPO_SOLIC TS,              '
      '      TB65_HIST_SOLICIT B,             '
      '      TB07_ALUNO A,                    '
      '      TB01_CURSO C,'
      '      TB08_MATRCUR M,'
      #9'  TB06_TURMAS TUR, TB129_CADTURMAS CT'
      ' Where SA.CO_ALU = A.CO_ALU'
      #9'AND SA.CO_CUR = C.CO_CUR'
      #9'AND SA.CO_TUR = TUR.CO_TUR'
      'AND CT.CO_TUR = TUR.CO_TUR'
      #9'AND SA.CO_SOLI_ATEN = B.CO_SOLI_ATEN'
      #9'AND TS.CO_TIPO_SOLI = B.CO_TIPO_SOLI'
      #9'AND A.CO_EMP = 2'
      #9'AND C.CO_EMP = 2'
      #9'AND B.CO_EMP = 2'
      #9'AND SA.CO_SIT = '#39'A'#39
      #9'AND A.CO_ALU = 1296'
      #9'AND M.CO_EMP = 2'
      #9'AND M.CO_ALU = A.CO_ALU'
      #9'AND M.CO_CUR = C.CO_CUR'
      'ORDER BY SA.CO_CUR, B.CO_TIPO_SOLI, SA.DT_SOLI_ATEN ')
    Left = 187
    Top = 0
  end
  inherited QryCabecalhoRel: TADOQuery
    Left = 234
    Top = 0
  end
  object qryAvaliacoes: TADOQuery
    Connection = DataModuleSGE.Conn
    CursorType = ctStatic
    LockType = ltBatchOptimistic
    Parameters = <>
    SQL.Strings = (
      'SET LANGUAGE PORTUGUESE                '
      ' Select  SA.DE_OBS_SOLI, A.NU_NIS,SA.CO_SOLI_ATEN, '
      '         SA.MES_SOLI_ATEN,'
      '         SA.ANO_SOLI_ATEN,'
      '         SA.NU_DCTO_SOLIC,'
      '         SA.DT_SOLI_ATEN,'
      '         B.CO_CUR,'
      '         CT.CO_SIGLA_TURMA as NO_TUR,'
      '         B.CO_TIPO_SOLI,'
      '         SA.DT_PREV_ENTR,              '
      '         B.DT_FIM_SOLI,                '
      '         A.NO_ALU, C.NO_CUR, C.CO_SIGL_CUR,'
      '         TS.NO_TIPO_SOLI,              '
      '         B.VA_SOLI_ATEN, '
      '         B.CO_SITU_SOLI CO_SIT_SOLI,'
      '         M.CO_ALU_CAD'
      ' From TB64_SOLIC_ATEND SA,             '
      '      TB66_TIPO_SOLIC TS,              '
      '      TB65_HIST_SOLICIT B,             '
      '      TB07_ALUNO A,                    '
      '      TB01_CURSO C,'
      '      TB08_MATRCUR M,'
      #9'  TB06_TURMAS TUR, TB129_CADTURMAS CT'
      ' Where SA.CO_ALU = A.CO_ALU'
      #9'AND SA.CO_CUR = C.CO_CUR'
      #9'AND SA.CO_TUR = TUR.CO_TUR'
      'AND CT.CO_TUR = TUR.CO_TUR'
      #9'AND SA.CO_SOLI_ATEN = B.CO_SOLI_ATEN'
      #9'AND TS.CO_TIPO_SOLI = B.CO_TIPO_SOLI'
      #9'AND A.CO_EMP = 2'
      #9'AND C.CO_EMP = 2'
      #9'AND B.CO_EMP = 2'
      #9'AND SA.CO_SIT = '#39'A'#39
      #9'AND A.CO_ALU = 1296'
      #9'AND M.CO_EMP = 2'
      #9'AND M.CO_ALU = A.CO_ALU'
      #9'AND M.CO_CUR = C.CO_CUR'
      'ORDER BY SA.CO_CUR, B.CO_TIPO_SOLI, SA.DT_SOLI_ATEN ')
    Left = 347
    Top = 8
  end
  object qryOcorrencias: TADOQuery
    Connection = DataModuleSGE.Conn
    CursorType = ctStatic
    LockType = ltBatchOptimistic
    Parameters = <>
    SQL.Strings = (
