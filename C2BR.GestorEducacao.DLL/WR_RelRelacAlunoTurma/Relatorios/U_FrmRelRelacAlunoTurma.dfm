  inherited QryRelatorio: TADOQuery
    CursorType = ctStatic
    SQL.Strings = (
      
        ' Select DISTINCT  MT.CO_ALU_CAD, mt.co_turn_mat, A.NO_ALU, A.TP_' +
        'RACA, A.TP_DEF, A.CO_SEXO_ALU , A.DT_NASC_ALU, MT.DT_CAD_MAT, MT' +
        '.CO_SIT_MAT, MT.CO_TUR, MT.CO_CUR,m.de_modu_cur,'#9
      '(SELECT CT.CO_SIGLA_TURMA FROM TB129_CADTURMAS CT'
      
        'WHERE MT.CO_TUR = CT.CO_TUR AND MT.CO_MODU_CUR = CT.CO_MODU_CUR ' +
        ')AS TURMA,  '
      '(SELECT CU.NO_CUR FROM TB01_CURSO AS CU                   '#9#9' '
      
        'WHERE MT.CO_CUR = CU.CO_CUR AND CU.CO_MODU_CUR = MT.CO_MODU_CUR ' +
        ')AS SERIE                        '
      
        'FROM TB07_ALUNO A                                               ' +
        '        '
      'LEFT OUTER JOIN TB08_MATRCUR MT ON                            '
      'MT.CO_ALU = A.CO_ALU AND                                      '
      'MT.CO_EMP = A.CO_EMP                              '
      'JOIN TB44_MODULO M ON MT.CO_MODU_CUR = M.CO_MODU_CUR '#9#9#9
      
        'INNER JOIN TB48_GRADE_ALUNO GA ON                               ' +
        '     '
      'GA.CO_ALU = MT.CO_ALU AND                                     '
      'GA.CO_EMP = MT.CO_EMP AND                                     '
      'GA.CO_CUR = MT.CO_CUR AND                                     '
      
        'GA.CO_EMP = 187 AND GA.CO_MODU_CUR = 1 AND GA.CO_CUR = 2 AND GA.' +
        'CO_TUR = 2 '
      'WHERE CO_SIT_MAT IN ('#39'A'#39','#39'P'#39') '
      'ORDER BY MT.CO_CUR, MT.CO_TUR , A.NO_ALU ')
