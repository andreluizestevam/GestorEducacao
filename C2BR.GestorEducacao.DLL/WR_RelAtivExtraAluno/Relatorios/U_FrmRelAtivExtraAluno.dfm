  inherited QryRelatorio: TADOQuery
    SQL.Strings = (
      
        'select distinct a.no_alu, a.nu_nire, a.co_sexo_alu, a.dt_nasc_al' +
        'u, mo.de_modu_cur, c.no_cur,ct.no_turma,ata.DT_CAD_ATIV,ata.QT_M' +
        'ES_ATIV,'
      'ae.DES_ATIV_EXTRA'
      'from tb08_matrcur mm'
      'join tb07_aluno a on a.co_alu = mm.co_alu'
      'join tb106_ativextra_aluno ata on ata.co_alu = a.co_alu'
      
        'join tb105_atividades_extras ae on ae.CO_ATIV_EXTRA = ata.CO_ATI' +
        'V_EXTRA'
      'join tb44_modulo mo on mo.co_modu_cur = mm.co_modu_cur'
      
        'join tb01_curso c on c.co_cur = mm.co_cur and c.co_emp = mm.co_e' +
        'mp'
      
        'join tb06_turmas t on t.co_tur = mm.co_tur and t.co_cur = mm.co_' +
        'cur and t.co_emp = mm.co_emp'
      'join tb129_cadturmas ct on t.co_tur = ct.co_tur'
      'where mm.co_emp = 187'
      'and mm.co_ano_mes_mat = 2009'
      'and mm.co_modu_cur = 1'
      'order by ae.des_ativ_extra')
