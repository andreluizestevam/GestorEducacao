  inherited QryRelatorio: TADOQuery
    CursorType = ctStatic
    SQL.Strings = (
      'SET LANGUAGE PORTUGUESE     '
      
        'SELECT Distinct  A.CO_ALU,MM.CO_ALU_CAD,T.CO_TUR,C.CO_CUR,A.NU_P' +
        'EC_DESBOL,A.NO_ALU,A.FLA_BOLSISTA,A.DT_VENC_BOLSA,A.DT_VENC_BOLS' +
        'AF,             '
      
        'A.NU_PEC_DESBOL,A.DE_TIPO_BOLSA,TU.NO_TURMA [NO_TUR],           ' +
        '        '
      
        'C.NO_CUR,MM.VL_TOT_MODU_MAT,MM.VL_DES_MOD_MAT,MM.VL_PAR_MOD_MAT,' +
        '              '
      'M.DE_MODU_CUR,R.NU_CPF_RESP,MM.CO_ANO_MES_MAT   '
      'FROM TB08_MATRCUR MM'
      
        'JOIN TB07_ALUNO A ON MM.CO_ALU = A.CO_ALU AND MM.CO_EMP = A.CO_E' +
        'MP'
      
        'JOIN TB06_TURMAS T ON MM.CO_TUR = T.CO_TUR AND MM.CO_EMP = T.CO_' +
        'EMP'
      
        'JOIN TB129_CADTURMAS TU ON MM.CO_TUR = TU.CO_TUR AND MM.CO_EMP =' +
        ' T.CO_EMP'
      
        'JOIN TB01_CURSO C ON MM.CO_CUR = C.CO_CUR AND MM.CO_EMP = C.CO_E' +
        'MP'
      'JOIN TB108_RESPONSAVEL R ON A.CO_RESP = R.CO_RESP        '
      'JOIN TB44_MODULO M ON MM.CO_MODU_CUR = MM.CO_MODU_CUR  '
      
        'WHERE A.FLA_BOLSISTA = '#39'S'#39'  AND  MM.CO_EMP = 2 AND MM.CO_CUR = 1' +
        ' AND MM.CO_ANO_MES_MAT = '#39'2008  '#39' '
      'AND MM.CO_TUR = 5 '
      'ORDER BY C.NO_CUR, T.NO_TUR, A.NO_ALU')
