  inherited QryRelatorio: TADOQuery
    SQL.Strings = (
      
        'SELECT a.co_alu, mm.co_cur,cu.no_cur,CU.CO_SIGL_CUR, mm.co_alu_c' +
        'ad, a.nu_nis, a.no_alu, ct.no_turma as no_tur, RE.nu_cpf_RESP, a' +
        '.dt_nasc_alu, a.co_sexo_alu, a.tp_raca, a.tp_def, a.renda_famili' +
        'ar, e.coduf, c.no_cidade, b.no_bairro,'
      #9'BE = (Case a.FLA_BOLSA_ESCOLA'
      #9#9#9'when '#39'False'#39' then '#39'N'#227'o'#39
      #9#9#9'when '#39'True'#39' then '#39'Sim'#39
      #9#9'  End),'
      #9'PASSE = (Case a.FLA_PASSE_ESCOLA'
      #9#9#9#9'when '#39'False'#39' then '#39'N'#227'o'#39
      #9#9#9#9'when '#39'True'#39' then '#39'Sim'#39
      #9#9#9' End)'
      'from tb07_aluno a'
      #9'JOIN tb905_bairro b on b.co_bairro = a.co_bairro'
      #9'JOIN tb74_UF e on e.coduf = a.co_esta_alu'
      #9'JOIN tb904_cidade c on c.co_cidade = a.co_cidade'
      
        '    JOIN tb08_matrcur mm on a.co_alu = mm.co_alu and a.co_emp = ' +
        'mm.co_emp'
      
        '    JOIN tb01_curso cu on mm.co_cur = cu.co_cur and mm.co_emp = ' +
        'cu.co_emp'
      #9'JOIN TB06_TURMAS TUR ON TUR.CO_TUR = MM.CO_TUR'
      'JOIN TB129_CADTURMAS CT ON TUR.CO_TUR = CT.CO_TUR'
      #9'JOIN TB108_RESPONSAVEL RE ON RE.CO_RESP = A.CO_RESP')
  end
